<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@drawable/iuet"
        android:label="Integral Attendance System"
        android:roundIcon="@drawable/iuet"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.anzer.attendancesystem.Reports_Detail_Activity"
            android:theme="@style/AppTheme2" />
        <activity android:name="com.anzer.attendancesystem.Reports_Activity" />
        <activity
            android:name="com.anzer.attendancesystem.ClassDetail_Activity"
            android:theme="@style/FullScreenTheme" />
        <activity
            android:name="com.anzer.attendancesystem.Insert_class_Activity"
            android:theme="@style/AppTheme2" />
        <activity android:name="com.anzer.attendancesystem.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>